<body>

  <header>
    <div class="container">
      <%- include('parciales/navigation') %>  
    </div>
  </header>
  <br>
  <div class="container">
    <h1>Alquiler de Departamentos</h1>
  </div>
  <br>
  <div class="container-fluid row">
    <aside class="col-md-3">
     <%- include('parciales/aside_Alquiler') %>
     </aside>
    <section class="col-md-9">
     <%- include('parciales/targets_Alquiler') %>

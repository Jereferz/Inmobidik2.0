  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
  <script>
    // Agrega un event listener para el evento "input" del rango
    document.getElementById("rangoDesde").addEventListener("input", function() {
        document.getElementById("valorRangoDesde").textContent = this.value;
    });
    document.getElementById("rangoHasta").addEventListener("input", function() {
        document.getElementById("valorRangoHasta").textContent = this.value;
    });
  </script>
